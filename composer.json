{
  "name": "kozz/best2pay",
  "description": "Best2Pay integration",
  "keywords": [
    "best2pay",
    "composer",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yury Kozyrev",
      "email": "urakozz@gmail.com"
    }
  ],
  "type": "project",
  "require": {
    "php": "~5.5|~7.0",
    "illuminate/support": "~5.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.*"
  },
  "suggest": {
    "kozz/laravel-guzzle-provider": "Suggested for loading Guzzle from the application service registry (~6.0)"
  },
  "autoload": {
    "psr-4": {
      "Kozz\\Best2Pay\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Kozz\\Best2Pay\\Laravel\\Providers\\Best2PayServiceProvider"
      ]
    }
  }
}
